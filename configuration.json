{
    "settings" : {
        "defaultSheet" : "vim",
        "shortcut" : "alt+d",
        "bgColors" : ["SkyBlue1", "SkyBlue2", "SkyBlue3"],
        "cleanKey" : "<Escape>",
        "backKey" : "<Ctrl-Escape>",
        "Debug" : true,
        "opacity" : 0.9,
        "position" : [0.2, 0.2],
        "HeadlineFont" : "Helvetica 15 bold",
        "AllowOverwrite" : true,
        "multiLineEntry" : false,
        "maxEntrys": 50,
        "columns" : 2
    },
    "crawler" : {
        "use" : true,
        "extension" : ".csh",
        "recrusive" : false,
        "directories" : [
            "./sheets"
        ]
    },
    "sheets" : {
        "vim" : "vim.csh",
        "python" : "python.csh"
    }
}
